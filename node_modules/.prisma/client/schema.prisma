generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Tarefa {
  id              Int       @id @default(autoincrement())
  titulo          String
  descricao       String?
  status          String
  data_vencimento DateTime?
  created_at      DateTime  @default(now())
  updated_at      DateTime  @updatedAt
}
